{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ui-adk",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "dev"
      ],
      "cwd": "${workspaceFolder}/ui-adk",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "serverReadyAction": {
        "pattern": "- Local:.+(https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    },
    {
      "name": "Debug ADK Agent",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/agents-python/agent-adk/agent_server_runner.py",
      "cwd": "${workspaceFolder}/agents-python/agent-adk",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ],
  "compounds": [
    {
      "name": "Launch Full Stack",
      "configurations": [
        "Debug ADK Agent",
        "Debug ui-adk"
      ]
    }
  ]
}